{
    "name": "lftn01/sin-ticketing-clientphp8",
    "description": "Cliente Sin-Ticketing php8",
    "license": "MIT",
    "keywords": ["laravel", "sin-ticketing", "api","client"],
    "autoload": {
        "psr-4": {
            "Hillus\\SinTicketingClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hillus\\SinTicketingClient\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Hillus Hilbert",
            "email": "hillus.hilbert@codding.com.br"
        },
        {
            "name": "Luiz Felipe",
            "email": "felipe.neves@codding.com.br"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.9.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        },
        "laravel": {
            "providers": [
                "Hillus\\SinTicketingClient\\ServiceProvider"
            ],
            "aliases": {
                "SinTicketingClient": "Hillus\\SinTicketingClient\\Facade"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^4|^5|^6"
    }
}
